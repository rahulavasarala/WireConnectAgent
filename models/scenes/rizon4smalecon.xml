                        <geom mesh="male-connector-minimal_collision_2" rgba="0.14830229256383265 0.6475927777174105 0.7338673709396254 1" class="collision"/>
                        <geom mesh="male-connector-minimal_collision_3" rgba="0.9669567561504457 0.1597578128209678 0.014474842421785983 1" class="collision"/>
                        <geom mesh="male-connector-minimal_collision_4" rgba="0.6799236025959631 0.3699565607110832 0.5072143585472161 1" class="collision"/>
                        <geom mesh="male-connector-minimal_collision_5" rgba="0.4268029638925618 0.48928493834570574 0.5899333271522715 1" class="collision"/>
                        <geom mesh="male-connector-minimal_collision_6" rgba="0.8260291399072678 0.4209205902022757 0.8043892236206673 1" class="collision"/>
                        <geom mesh="male-connector-minimal_collision_7" rgba="0.6130379479462504 0.42045031701196356 0.540917958831619 1" class="collision"/>
